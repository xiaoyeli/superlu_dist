/* superlu_dist_config.h.in */

/* Enable CUDA */
#cmakedefine HAVE_CUDA @HAVE_CUDA@

/* Enable NVSHMEM */
#cmakedefine HAVE_NVSHMEM @HAVE_NVSHMEM@

/* Enable HIP */
#cmakedefine HAVE_HIP @HAVE_HIP@

/* Enable parmetis */
#cmakedefine HAVE_PARMETIS @HAVE_PARMETIS@

/* Enable colamd */
#cmakedefine HAVE_COLAMD @HAVE_COLAMD@

/* Enable LAPACK */
#cmakedefine SLU_HAVE_LAPACK @SLU_HAVE_LAPACK@

/* Enable CombBLAS */
#cmakedefine HAVE_COMBBLAS @HAVE_COMBBLAS@

/* Enable MAGMA */
#cmakedefine HAVE_MAGMA @HAVE_MAGMA@

/* enable 64bit index mode */
#cmakedefine XSDK_INDEX_SIZE @XSDK_INDEX_SIZE@

#if defined(XSDK_INDEX_SIZE) && (XSDK_INDEX_SIZE == 64)
#define _LONGINT 1
#endif
